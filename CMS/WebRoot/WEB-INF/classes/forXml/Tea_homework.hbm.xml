<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="forXml">
    <class name="Tea_homework" table="tea_homework">
        <composite-id name="pkey" class="pkeyTea_homework">
            <key-property name="course_no" column="course_no"></key-property>
            <key-property name="homework_no" column="homework_no"></key-property>
        </composite-id>
    </class>
     
</hibernate-mapping>